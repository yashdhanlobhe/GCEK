<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivityWithoutLogin">
    <androidx.appcompat.widget.Toolbar
        android:id="@+id/MainActivityWTOToolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:foregroundGravity="center"
        android:background="@color/s1"
    >
        <TextView
            android:gravity="center"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="G  C  E  K"
            android:textColor="@color/white"
            android:textSize="22dp"
            android:textStyle="bold"/>
    </androidx.appcompat.widget.Toolbar>


    <FrameLayout
        android:layout_below="@+id/MainActivityWTOToolbar"
        android:id="@+id/MainActivityWTOFrameLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/MainActivityWTOBottomNavView"
        />


    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:layout_width="match_parent"
        android:elevation="5dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:background="?android:attr/windowBackground"
        app:menu="@menu/nav_item_for_without_login"
        android:id="@+id/MainActivityWTOBottomNavView"/>
</RelativeLayout>